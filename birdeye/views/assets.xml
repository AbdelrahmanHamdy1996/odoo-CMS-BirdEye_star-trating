<odoo>
    <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/birdeye/static/src/scss/style.scss"/>
        </xpath>
    </template>
    <template id="my_js" inherit_id="website.assets_frontend" name="My Js">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/birdeye/static/src/js/StarRating.js"/>
        </xpath>
    </template>

</odoo>